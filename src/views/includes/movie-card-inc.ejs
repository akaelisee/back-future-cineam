<% if (results.length) { %>
    <% results.data.forEach(result => { %>
        <div class="col-lg-3">
            <div href="#" class="card">
                <div class="card-header">
                    <ul class="list-unstyled list-info">
                        <li>
                            <div class="pdd-vertical-10 pdd-horizon-20">
                                <img  class="thumb-img img-circle" src="<%= result.poster_path %>" alt="">
                                <div class="info">
                                    <span class="title"><%= result.title %></span>
                                    <span class="sub-title">
                                        <i class="fas fa-sign"></i>
                                        <span><%= result.genre %></span>
                                    </span>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
                <div class="card-media" style="padding: 0 0px;">
                    <img style="height: 450px; width: 100%;" class="img-responsive" src="<%= result.poster_path %>" alt="">
                </div>
                <div class="card-footer border top">
                    <ul class="list-unstyled list-inline pull-right">
                        <li class="list-inline-item">
                            <a href="/admin/account/detail-movie/<%= result.id %>" class="btn btn-icon btn-flat btn-rounded">
                                <i class="fas fa-eye"></i>
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="/admin/account/edit-movies/<%= result.id %>" class="btn btn-icon btn-flat btn-rounded agencydelter">
                                <i class="fas fa-pen"></i>
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="/admin/account/delete-movie/<%= result.id %>" class="btn btn-icon btn-flat btn-rounded agencydelter">
                                <i class="fas fa-trash"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    <% }) %> 
    <% } else { %>
     <p> Pas de films actuellement </p>
<% } %>